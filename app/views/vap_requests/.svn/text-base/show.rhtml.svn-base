  	<tr><td valign="top">Days Selected:</td><td align="right">
  	  <% @vap_request.vap_days.each do |day| -%>
  	    <%= day.date.strftime("%a, %b. %e") %><br />
  	  <% end -%>
  	</td></tr>
  	</table>
  </td>
  </tr>
